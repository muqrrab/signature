<?xml version="1.0" encoding="UTF-8"?>
<configuration>

    <appSettings>
    <!-- Required settings -->
    <add key="WSGI_HANDLER" value="django.core.wsgi.get_wsgi_application()" />
    <!-- Your django path -->
    <add key="PYTHONPATH" value="D:\SignatureApp" /> 
    <!-- Your djangoname.settings -->
    <add key="DJANGO_SETTINGS_MODULE" value="signature.settings" />
  </appSettings>
    <system.webServer>
        <handlers>
            <add name="signature" path="*" verb="*" modules="FastCgiModule" scriptProcessor="D:\SignatureApp\env\Scripts\python.exe|D:\SignatureApp\env\Lib\site-packages\wfastcgi.py" resourceType="Unspecified" />
        </handlers>
    </system.webServer>
</configuration>
